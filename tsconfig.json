{
  "compilerOptions": {
    "declaration": true,
    "noUnusedLocals": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "allowSyntheticDefaultImports": true,
    "outDir": "dist",
    "sourceMap": true,
    "target": "es5",
    "lib": [
      "dom",
      "es2015"
    ]
  },
  "include": [
    "src",
    "typings"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ]
}
